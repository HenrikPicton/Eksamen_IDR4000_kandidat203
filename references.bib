  author = {Siedler, Madelin R. and Rodriguez, Christian and Stratton, Matthew T. and Harty, Patrick S. and Keith, Dale S. and Green, Jacob J. and Boykin, Jake R. and White, Sarah J. and Williams, Abegale D. and DeHaven, Brielle and Tinsley, Grant M.},
  year = 2023,
  month = sep,
  journal = {British Journal of Nutrition},
  volume = {130},
  number = {5},
