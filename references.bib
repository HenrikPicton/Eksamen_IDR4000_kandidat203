@article{tromarasLeanBodyMass2024,
  title = {Lean {{Body Mass}}, {{Muscle Architecture}} and {{Powerlifting Performance}} during {{Preseason}} and in {{Competition}}},
  author = {Tromaras, Konstantinos and Zaras, Nikolaos and Stasinaki, Angeliki-Nikoletta and Mpampoulis, Thomas and Terzis, Gerasimos},
  year = 2024,
  month = may,
  journal = {Journal of Functional Morphology and Kinesiology},
  volume = {9},
  number = {2},
  pages = {89},
  issn = {2411-5142},
  doi = {10.3390/jfmk9020089},
  urldate = {2026-01-02},
  abstract = {Lean body mass (LBM) is correlated with powerlifting performance in athletes competing in different bodyweight classes. However, it remains unknown whether changes in LBM are correlated with performance changes in powerlifters preparing for a competition. The aim of this study was to investigate the changes in LBM and performance in powerlifters preparing for a competition. Eight male powerlifters (age 31.7 \textpm{} 9.8 years, height 1.77 \textpm{} 0.06 m, weight 99.2 \textpm{} 14.6 kg) and three female powerlifters (age 32.7 \textpm{} 16.3 years, height 1.54 \textpm{} 0.06 m, weight 66.6 \textpm{} 20.9 kg) participated in the study. The athletes followed individualized periodized training programs for 12 weeks, aiming to maximize their performance for the national championship. The maximum strength (1-RM) in the squat, bench press, and deadlift, body composition, handgrip strength, anaerobic power, quadriceps' cross-sectional area and vastus lateralis muscle architecture were measured before and after the training period. Significant increases were found after the training period in the squat (5.8 \textpm{} 7.0\%, p {$<$} 0.05), bench press (4.9 \textpm{} 9.8\%, p = 0.05) and deadlift (8.3 \textpm{} 16.7\%, p {$<$} 0.05). Significant correlations were found between the 1-RM and LBM before and after the training period (r {$>$} 0.75, p {$<$} 0.05). The changes in the 1-RM after the training intervention correlated with the changes in the total LBM (p {$<$} 0.05). These results suggest that individual changes in LBM due to systematic resistance training for a competition may dictate increases in the 1-RM strength in powerlifters.},
  langid = {english},
  file = {C:\Users\blabe\Zotero\storage\KKBDQTYJ\Tromaras et al. - 2024 - Lean Body Mass, Muscle Architecture and Powerlifting Performance during Preseason and in Competition.pdf}
}
@article{fonsecaComparisonTraditionalAdvanced2023,
  title = {Comparison of {{Traditional}} and {{Advanced Resistance Training Paradigms}} on {{Muscle Hypertrophy}} in {{Trained Individuals}}: {{A Systematic Review}} and {{Meta-Analysis}}},
  shorttitle = {Comparison of {{Traditional}} and {{Advanced Resistance Training Paradigms}} on {{Muscle Hypertrophy}} in {{Trained Individuals}}},
  author = {Fonseca, Pedro A. B. and Ide, Bernardo N. and Oranchuk, Dustin J. and Marocolo, Moacir and Simim, M{\'a}rio A. M. and Roberts, Michael D. and Mota, Gustavo R.},
  editor = {{Mackey-Sennels}, Abigail},
  year = 2023,
  month = jul,
  journal = {Translational Sports Medicine},
  volume = {2023},
  pages = {1--15},
  issn = {2573-8488},
  doi = {10.1155/2023/9507977},
  urldate = {2026-01-02},
  abstract = {Trained individuals may require variations in training stimuli and advanced resistance training paradigms (ADV) to increase skeletal muscle hypertrophy. However, no meta-analysis has examined how ADV versus traditional (TRAD) approaches may differentially affect hypertrophic outcomes in trained populations. The aim of this review was to determine whether the skeletal muscle hypertrophy responses induced by TRAD differed from ADV in resistance-trained individuals. Furthermore, we sought to examine potential effects of dietary factors, participants' training status, and training loads. We searched for peer-reviewed, randomized controlled trials (published in English) conducted in healthy resistance-trained adults performing a period of TRAD and ADV with pre-to-post measurement(s) of muscle hypertrophy in PubMed, Web of Science, SPORTDiscus, and MEDLINE databases up to October 2022. A formal meta-analysis was conducted in Revman5, and risk of bias was assessed by ROB2. Ten studies met the inclusion criteria. Results indicated no difference between ADV and TRAD for muscle thickness (SMD\,=\,0.05, 95\% CI: -0.20 0.29,                                                   p                                               \,=\,0.70), lean mass (SMD\,=\,-0.01, 95\% CI: -0.26 0.23,                                                   p                                               \,=\,0.92), muscle cross-sectional area (SMD\,=\,-0.07, 95\% CI: -0.36 0.22,                                                   p                                               \,=\,0.64), or all measurements analyzed together (SMD\,=\,-0.00, 95\% CI: -0.15 0.14,                                                   p                                               \,=\,0.95). No heterogeneity or inconsistencies were observed; however, unclear risk of bias was present in most of the studies. Short-term ADV does not induce superior skeletal muscle hypertrophy responses when compared with TRAD in trained individuals. This review was not previously registered.},
  copyright = {https://creativecommons.org/licenses/by/4.0/},
  langid = {english}
}
